.nav
	%a.logo{:href => "/"}
	.nav-right
		.nav-top
			%a.live-chat{:href => "javascript:void(0);", :onclick => "habla_window.expand()"}
				
			%a.contact{:href => "/contact"}
			.toll-free
				%img.phone{:src => "/images/phone.png"}
				%h4
					CALL TOLL FREE:
				%span
					866.948.5100			
					
		.nav-bottom
			.nav-tabs
				- $tabs.each do |tab,url|
					
					- cl = tab
					- cl += (@current_page==tab ? " current" : "")
					- cl += ($tabs.first==[tab,url] ? " first" : "")
					- cl += ($tabs.last==[tab,url] ? " last" : "")
					%a.tab{:class => "#{cl}", :id => "#{tab}", :href => "#{url}"}
						%span{:class => "#{tab}"} #{tab.capitalize}